# frozen_string_literal: true
require 'yahns/rackup_handler'
ARGV.grep(/\A(?:-s|--server)/)[0] and abort "--server has no effect for #$0"
ARGV[0, 0] = %w(-s yahns)
Rack::Handler.register('yahns', 'Yahns::RackupHandler')
Rack::Server.start
